@model YourVitebskWebServiceApp.ViewModels.UserViewModel
@{
    ViewData["Title"] = "Удаление пользователя";
}
<h2>@ViewBag.Title</h2>
<div>
    <dl class="dl-horizontal">
        <dt>Имя</dt>
        <dd>
            @Model.UserId
        </dd>

        <dt>Фамилия</dt>
        <dd>
            @Model.Email
        </dd>

        <dt>Email</dt>
        <dd>
            @Model.FirstName
        </dd>
 
        <dt>Возраст</dt>
        <dd>
            @Model.Phone
        </dd>
    </dl>
 
    <div>
        <form asp-controller="Home" asp-action="Delete" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>